#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main() {
    int N, Q;
    cin >> N >> Q;

    string S;
    cin >> S;

    while (Q--) {
        string query;
        cin >> query;

        if (query == "pop_back") {
            // Remove the last character
            S.pop_back();
        } else if (query == "front") {
            // Print the first character
            cout << S.front() << '\n';
        } else if (query == "back") {
            // Print the last character
            cout << S.back() << '\n';
        } else if (query == "push_back") {
            // Add character x to the end of the string
            char x;
            cin >> x;
            S.push_back(x);
        } else if (query == "print") {
                int pos;
                cin >> pos;
                // Print character at position pos
                cout << S[pos - 1] << '\n';
        }else {
            int l, r;
            cin >> l >> r;

            if (query == "sort") {
                // Sort characters from l to r
                sort(S.begin() + l - 1, S.begin() + r);
            } else if (query == "reverse") {
                // Reverse characters from l to r
                reverse(S.begin() + l - 1, S.begin() + r);
            } else if (query == "substr") {
                // Print substring from l to r
                cout << S.substr(l - 1, r - l + 1) << '\n';
            }
        }
    }

    return 0;
}
