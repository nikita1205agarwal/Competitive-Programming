#include <iostream>
using namespace std;

int main() {
    int N, Q;

    cin >> N >> Q;

    int A[N+1]; 
    int cumulativeSum[N+1] = {0};

    for (int i = 1; i <= N; i++) {
        cin >> A[i];
    }

    for (int i = 1; i <= N; i++) {
        cumulativeSum[i] = cumulativeSum[i - 1] + A[i];
    }

    for (int q = 1; q <= Q; q++) {
        int L, R;

        cin >> L >> R;

        int summation = cumulativeSum[R] - cumulativeSum[L - 1];

        cout << summation << '\n';
    }

    return 0;
}
