#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int N;
    cin >> N;

    int A[N][M]; 
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            cin >> A[i][j];
        }
    }

    int primaryDiagonalSum = 0;
    int secondaryDiagonalSum = 0;

    for (int i = 0; i < N; i++) {
        primaryDiagonalSum += A[i][i];
        secondaryDiagonalSum += A[i][N - 1 - i];
    }

    int absoluteDifference = abs(primaryDiagonalSum - secondaryDiagonalSum);

    cout << absoluteDifference << '\n';

    return 0;
}
