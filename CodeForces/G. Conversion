#include <iostream>
using namespace std;

int main() {
    string S;
    cin >> S;

    for (int i=0;i<S.size();i++) {
        if (S[i] == ',') {
            S[i] = ' ';
        }
        else if (isupper(c)) {
            c = tolower(c);
        }
        else
            c = toupper(c);
    }

    cout << S << '\n';

    return 0;
}
