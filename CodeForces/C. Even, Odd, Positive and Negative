#include<iostream>
using namespace std;
int main()
{
	int n;
	cin>>n;
	long even=0, odd=0, pos=0, neg=0;
	for(int i=1;i<=n;i++){
		long x;
		cin >>x;
		if(x%2==0)
			even++;
		else
			odd++;
		if(x<0)
			neg++;
		else if(x>0)
			pos++;
	}
	cout<<"Even: "<<even<<"\n";
	cout<<"Odd: "<<odd<<"\n";
	cout<<"Positive: "<<pos<<"\n";
	cout<<"Negative: "<<neg<<"\n";
}
