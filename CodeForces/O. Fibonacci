#include <iostream>

using namespace std;


int main() {
    int n;
    cin >> n;
    
     if (n == 1) cout<<0<<"\n";
     else if (n == 2) cout<<1<<"\n";
     else{
         int f=0, s=1, t=0;
         for(int i=0;i<=n/2;i++){
             t=f+s; 
             f=t;
             s=t+s;
         }
         if(n%2!=0)
            cout<<t<<"\n";
         else
            cout<<s<<"\n";
     }

    return 0;
}
