#include<iostream>
#include<cmath>
using namespace std;
int main()
{
	long a, b, c;
	if(a<b && a<c){
	    cout<<a<<"\n";
	    if(b<c)
	        cout<<b<<"\n";
	    else
	        cout<<c<<"\n";
	}
	else if(b<c){
	    cout<<b<<"\n";
	    if(a<c)
	        cout<<a<<"\n";
	    else
	        cout<<c<<"\n";
	}
	else{
	    cout<<c<<"\n";
	    if(a<b)
	        cout<<a<<"\n";
	    else
	        cout<<b<<"\n";
	}
	cout<<"a"<<"\n"<<b<<"\n"<<c<<"\n";
}
