#include <iostream>
#include <string>
using namespace std;

int main() {
    int N;
    cin >> N;

    while (N--) {
        string S, T;
        cin >> S >> T;

        string result = "";

        int i = 0, j = 0;

        while (i < S.length() || j < T.length()) {
                result += S[i];
                i++;
                result += T[j];
                j++;
        }
        if(i < S.length()){
            for(;i<i < S.length();i++)
                result += S[i];
        }
        if(j < T.length()){
            for(;j < T.length();j++)
                result += T[j];
        }
        cout << result << '\n';
    }

    return 0;
}
