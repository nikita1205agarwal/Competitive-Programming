#include <iostream>

using namespace std;

int main() {
    int N;
    cin >> N;

    int A[N];

    for (int i = 0; i < N; ++i) {
        cin >> A[i];
    }

    int maxOperations = 0;
    bool allEven = true;

    for (int i = 0; i < N; ++i) {
        if (A[i] % 2 != 0) {
            allEven = false;
            break;
        }
    }

    while (allEven) {
        for (int i = 0; i < N; ++i) {
            A[i] /= 2;
            if (A[i] % 2 != 0) {
                allEven = false;
                break;
            }
        }
        if (allEven) {
            maxOperations++;
        }
    }

    cout << maxOperations << "\n";

    return 0;
}
