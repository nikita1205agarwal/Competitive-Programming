#include<iostream>
using namespace std;
int main()
{
	int n;
	while(true){
	    cin>>n;
	    if(n==1999){
	        cout<<"Correct\n";
	        break;
	    }
	    else
	        cout<<"Wrong\n";
	}
	cout<<"Even: "<<even<<"\n";
	cout<<"Odd: "<<odd<<"\n";
	cout<<"Positive: "<<pos<<"\n";
	cout<<"Negative: "<<neg<<"\n";
}
