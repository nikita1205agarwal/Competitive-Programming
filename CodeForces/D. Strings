#include <iostream>
using namespace std;

int main() {
    string A, B;

    cin >> A >> B;

    int sizeA = A.size();
    int sizeB = B.size();

    string concatAB = A + B;
    
    swap(A[0], B[0]);

    cout << sizeA << ' ' << sizeB << '\n';
    cout << concatAB << '\n';
    cout << A << ' ' << B << '\n';

    return 0;
}
